#variable
cc = clang-cl

# rule
rule obj
    command = $cc $cflags -c IOCompletionPort.cc -o IOCompletionPort.o

rule out
    command = $cc $cflags TCPServ-iocp_win10-x64.cc IOCompletionPort.o -o TCPServ-iocp.exe /link ws2_32.lib

# build outputs: rulename inputs
build IOCompletionPort.o: obj IOCompletionPort.cc
build TCPServ-iocp.exe: out TCPServ-iocp_win10-x64.cc IOCompletionPort.o

